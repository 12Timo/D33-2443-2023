/*
For loops for cubes of numbers
Author: Timothy Mwangi 
Reg No:D33-2443-2023
Date: 10th March 2025
*/

#include <stdio.h>

int main() {
    int start, stop, i;

    printf("Enter a starting value: \n");
    scanf("%d", &start);

    printf("Enter a stopping value:\n");
    scanf("%d", &stop);

        for (i = start; i <= stop; i++) {
            printf("Number: %d, Cube: %d\n", i, i * i * i);
        }
    

    return 0; // Successful execution
}
